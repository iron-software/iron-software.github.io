{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "bin/IronOcr/lib/netstandard2.0/IronOcr.dll",
            "bin/IronOcr/lib/net60/IronOcr.dll"
          ]
        }
      ],
      "dest": "api",
      "filter": "filterConfig.yml",
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md",
          "api/index.md"
        ]
      }
    ],
    "globalMetadata": {
      "_appTitle": "C# OCR & Tesseract API | IronOCR",
      "_enableSearch": true,
      "_appFooter": "<div style='text-align:center'><a class='nuget-cta' href='https://www.nuget.org/packages/IronOcr' target='_blank'><i class='nuget-icon'></i> Install with Nuget</a></div>"
    },
    "dest": "output/ironsoftware.com/csharp/ocr/object-reference/",
    "globalMetadataFiles": [
      "homepages/ironocr/version.json",
      "homepages/ironocr/customVariable.json"
    ],
    "template": [
      "templates/default",
      "templates/darkfx"
    ],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": true,
    "disableGitFeatures": false,
    "allowBrokenLinks": true
  }
}
